set terminal pdf enhanced size 4 in, 2 in
set output 'ecinc2.pdf'

#set title "Metadata Error Rate"
#set xrange [-0.01:1.02]
set yrange [0:1]
#set format x ""
set ylabel "Verified Rate"
set xlabel "Probability of an error per position (%)"
set key left bottom

fname="ecinc2.data"
plot \
	"< awk '$6 == 1 { print $0 }' ".fname using 8:9 with lines ls 1 title "EC 5",\
	"< awk '$6 == 2 { print $0 }' ".fname using 8:9 with lines ls 2 title "EC 10",\
	"< awk '$6 == 3 { print $0 }' ".fname using 8:9 with lines ls 3 title "EC 15",\
	"< awk '$6 == 4 { print $0 }' ".fname using 8:9 with lines ls 4 title "EC 20",\
	"< awk '$6 == 5 { print $0 }' ".fname using 8:9 with lines ls 5 title "EC 25",\
	"< awk '$6 == 6 { print $0 }' ".fname using 8:9 with lines ls 6 title "EC 30",\
	"< awk '$6 == 7 { print $0 }' ".fname using 8:9 with lines ls 7 title "EC 35",\
	"< awk '$6 == 8 { print $0 }' ".fname using 8:9 with lines ls 8 title "EC 40",\
